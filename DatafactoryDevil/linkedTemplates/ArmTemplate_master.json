{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DatafactoryDevil"
		},
		"datalake6767_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalake6767'"
		},
		"dataworkspace6767_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'dataworkspace6767'"
		},
		"datalake6767_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake666.dfs.core.windows.net/"
		},
		"dataworkspace6767_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dataworkspace7575.sql.azuresynapse.net"
		},
		"dataworkspace6767_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "datapoolRG"
		},
		"dataworkspace6767_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DatafactoryDevil_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalake6767_accountKey": {
						"value": "[parameters('datalake6767_accountKey')]"
					},
					"dataworkspace6767_password": {
						"value": "[parameters('dataworkspace6767_password')]"
					},
					"datalake6767_properties_typeProperties_url": {
						"value": "[parameters('datalake6767_properties_typeProperties_url')]"
					},
					"dataworkspace6767_properties_typeProperties_server": {
						"value": "[parameters('dataworkspace6767_properties_typeProperties_server')]"
					},
					"dataworkspace6767_properties_typeProperties_database": {
						"value": "[parameters('dataworkspace6767_properties_typeProperties_database')]"
					},
					"dataworkspace6767_properties_typeProperties_userName": {
						"value": "[parameters('dataworkspace6767_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}